{
  "name": "@prefer-jsr/eslint-plugin-prefer-jsr",
  "version": "0.2.1",
  "description": "ESLint plugin that suggests using JSR packages over NPM when available",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "jsr",
    "npm",
    "npm2jsr",
    "lint",
    "prefer-jsr"
  ],
  "homepage": "https://github.com/prefer-jsr/prefer-jsr#readme",
  "bugs": {
    "url": "https://github.com/prefer-jsr/prefer-jsr/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prefer-jsr/prefer-jsr.git",
    "directory": "packages/eslint-plugin"
  },
  "license": "MIT",
  "author": {
    "name": "prefer-jsr",
    "url": "https://github.com/prefer-jsr"
  },
  "type": "module",
  "exports": {
    ".": {
      "@prefer-jsr/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "dependencies": {
    "@prefer-jsr/npm2jsr": "workspace:*"
  },
  "devDependencies": {
    "@humanwhocodes/momoa": "^3.3.9",
    "eslint-plugin-eslint-plugin": "^7.0.0",
    "jsonc-eslint-parser": "^2.4.1"
  },
  "peerDependencies": {
    "@eslint/json": ">=0.13.0",
    "eslint": ">=9.15.0"
  },
  "peerDependenciesMeta": {
    "@eslint/json": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20.9.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "name": "eslint-plugin"
  }
}
