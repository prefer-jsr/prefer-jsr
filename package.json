{
  "name": "@prefer-jsr/source",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for prefer-jsr: ESLint tooling to suggest JSR packages over NPM",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "~8.57.0",
    "@nx/eslint": "21.6.4",
    "@nx/eslint-plugin": "21.6.4",
    "@nx/js": "21.6.4",
    "@nx/vite": "21.6.4",
    "@nx/web": "21.6.4",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.13.3",
    "@swc/helpers": "~0.5.11",
    "@types/node": "20.19.9",
    "@vitest/coverage-v8": "^3.0.5",
    "@vitest/ui": "^3.0.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-package-json": "^0.56.3",
    "eslint-plugin-perfectionist": "^4.15.1",
    "jiti": "2.4.2",
    "jsonc-eslint-parser": "^2.1.0",
    "knip": "^5.64.3",
    "nx": "21.6.4",
    "prettier": "^2.6.2",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.46.0",
    "verdaccio": "^6.0.5",
    "vite": "^7.1.11",
    "vitest": "^3.0.0"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd",
  "engines": {
    "node": ">=20.9.0"
  },
  "nx": {
    "targets": {
      "lint": {
        "executor": "nx:run-commands",
        "options": {
          "command": "eslint package.json"
        }
      },
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
