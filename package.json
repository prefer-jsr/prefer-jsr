{
  "name": "@prefer-jsr/source",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for prefer-jsr: ESLint tooling to suggest JSR packages over NPM",
  "license": "MIT",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@nx/eslint": "21.6.4",
    "@nx/eslint-plugin": "21.6.4",
    "@nx/js": "21.6.4",
    "@nx/vite": "21.6.4",
    "@swc-node/register": "~1.9.2",
    "@swc/core": "~1.13.21",
    "@swc/helpers": "~0.5.17",
    "@types/node": "20.19.9",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-dependabot": "^0.6.0",
    "eslint-plugin-package-json": "^0.56.4",
    "eslint-plugin-perfectionist": "^5.5.0",
    "eslint-yaml": "^0.1.0",
    "jiti": "2.4.2",
    "jsonc-eslint-parser": "^2.4.2",
    "jsr": "^0.13.5",
    "knip": "^5.83.1",
    "nx": "21.6.4",
    "prettier": "^2.8.8",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.50.0",
    "verdaccio": "^6.2.4",
    "vite": "^7.3.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.26.0+sha512.3b3f6c725ebe712506c0ab1ad4133cf86b1f4b687effce62a9b38b4d72e3954242e643190fc51fa1642949c735f403debd44f5cb0edd657abe63a8b6a7e1e402",
  "engines": {
    "node": ">=20.9.0"
  },
  "nx": {
    "targets": {
      "lint": {
        "executor": "nx:run-commands",
        "options": {
          "command": "eslint package.json .github/"
        }
      },
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
